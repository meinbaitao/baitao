package com.bt.modules.material.entity;

import com.thinkgem.jeesite.common.persistence.DataEntity;

/**
 * 可配置加工项物料信息与加工项Mapping Entity
 * @author zjs
 * @version 2016-05-06
 */
public class SettlementMaterialMapping extends DataEntity<SettlementMaterialMapping> {

	private static final long serialVersionUID = 1L;
	
	private String pid;
	private String settlementItem;
	private String materialName;
	private String materialNo;
	private String settlementType;
	
	public String getMaterialName() {
		return materialName;
	}
	public void setMaterialName(String materialName) {
		this.materialName = materialName;
	}
	public String getMaterialNo() {
		return materialNo;
	}
	public void setMaterialNo(String materialNo) {
		this.materialNo = materialNo;
	}
	public String getSettlementType() {
		return settlementType;
	}
	public void setSettlementType(String settlementType) {
		this.settlementType = settlementType;
	}
	public String getPid() {
		return pid;
	}
	public void setPid(String pid) {
		this.pid = pid;
	}
	
	public String getSettlementItem() {
		return settlementItem;
	}
	public void setSettlementItem(String settlementItem) {
		this.settlementItem = settlementItem;
	}
	
}
