public enum VariablesRoleEnum {
	VAR_PROJECT_RESPON("project_responsible_people"),//项目负责人
	VAR_AREA_MANAGER("area_manager"),//区域经理
	VAR_QUANLITY_SECUR("quality_security_office"),//质安员
	VAR_COST_RESPON("cost_responsible_person"),//成本负责人
	VAR_COST_LEADER("costing_leader"),//成本组长
