}

.list-content .nav-operate ul.search{
	float:right;
    height:45px;
    margin:2px 2px 2px 0;
	}

.list-content .nav-operate ul.search::after{
	content:"";
	clear:both;
}	

.list-content .nav-operate ul.search li{
    width:auto;
	height:34px;
	line-height:34px;
	font:14px/34px "\5FAE\8F6F\96C5\9ED1";
	margin:5px 6px; 
	}
	
.list-content .nav-operate ul.search li .search-btn{
    padding:0 16px;
	font:16px/34px "\5FAE\8F6F\96C5\9ED1";
	border:none;
	border-radius:5px;
	background-color:#414F63;
	color:#fff;
	}
.list-content .nav-operate ul.search li button.icon{padding-right:4px;}
.list-content .nav-operate ul.search li button.icon i{
	background:url(../jquery-select2/3.4/select2x2.png) no-repeat -11px -19px;
	display: inline-block;
    float: right;
    height: 10px;
    margin:14px 3px 10px;
    width: 12px;
	}
	
.list-content .nav-operate ul.search li.input input,
.list-content .nav-operate ul.search li.input div{
    margin:0px;
    vertical-align: middle;
	}
.list-content .nav-operate ul.search li.input>i{
	 font-style:normal;
	 display:none;
}
.list-content .nav-operate ul.search li.input>i input[type=text],
。list-content input[type=text]{
	display: inline-block;
    border-radius: 4px;
    background-color: #fff;
    border: 1px solid #ccc;
    color: #555;
    font-size: 14px;
    height: 20px;
    line-height: 20px;
    padding: 4px 6px;    
    vertical-align: middle;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    transition: border 0.2s linear 0s, box-shadow 0.2s linear 0s;
   
    }
 .list-content .nav-operate ul.search li.input>i input[type=text]:focus{
    border-color: rgba(82, 168, 236, 0.8);
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(82, 168, 236, 0.6);
    outline: 0 none;
    }
.list-content .nav-operate ul.search li.input i.input-show{
    display:inline-block;
	}
	
.list-content .nav-operate div.advanced-search-form li label>span,
.list-content .nav-operate ul.search li.input i.input-show>span{
    display: inline-block;
    margin:0 8px 0 0px;
}
