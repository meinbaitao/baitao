}

body>.pagination{
	position:fixed;
	right:1%;
	bottom:14px;
	margin:0px 0px 0px 0px;
	z-index: 5;
}

.pagination ul{
	float:right;
}

.pagination ul > .disabled > a, .pagination ul > .disabled > a:hover, .pagination ul > .disabled > a:focus {
    background-color: #f5f5f5;
    color: #999;
    cursor: default;
}
.pagination ul > .controls > a, .pagination ul > .controls > a:hover, .pagination ul > .controls > a:focus {
    background-color: #d2d2d2;
    color: #999;
    cursor: default;
    border-color: #ddd;
    border-image: none;
    border-style: solid;
    border-width: 1px 1px 1px 0;
}

