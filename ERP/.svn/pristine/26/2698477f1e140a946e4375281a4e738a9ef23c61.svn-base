body{
	width:100%;
	min-height:300px;
	background: #eff0f4 none repeat scroll 0 0;
	overflow:hidden;
	position:relative;
}
.form-content form.form-horizontal{

	background-color:#fff;
	padding:30px 0px;
}

@media screen and (max-width:960px){
.form-content  form.form-horizontal .control-group{
	  width:100%;
  }
.form-content  form.form-horizontal div.control-group-all label.control-label{
	min-width:160px;
	width:26%;
   }
.form-content  form.form-horizontal div.control-group-all div.controls{
	min-width:210px;
	width:65%;
   }
   
}

@media screen and (min-width:980px){
	
form.form-horizontal .control-group{
	min-width:425px;
	width:50%;
	float:left;
  }
  
}
form.form-horizontal .control-group{
	border:none;
  }
  
form.form-horizontal .control-group-all{
	width:100%;
}
form.form-horizontal div.control-group-all label.control-label{
	min-width:160px;
	width:13%;
}
form.form-horizontal div.control-group-all div.controls{
	min-width:560px;
	width:83%;
}
form.form-horizontal div.control-group-all div.controls input[type=text]{
	width:90%;
  }

form.form-horizontal .control-group .control-label{
	min-width:160px;
	width:26%;
  }
  
form.form-horizontal .control-group .controls{
	min-width:210px;
	width:60%;
  }

form.form-horizontal .control-group .controls input[type=text],
form.form-horizontal .control-group .controls input[type=password],
form.form-horizontal .control-group .controls select{
	min-width:200px;
	width:90%;
  }
form.form-horizontal .control-group .controls .input-append input[type=text],
ul.ul-form  div.input-append input[type=text]{
	min-width:180px;
	width:75%;
	border-radius:4px;
  }

.form-content  form.form-horizontal .form-actions::before{display:none;}	
.form-content  form.form-horizontal .form-actions{
	position:fixed;
	display:block;
	width:100%;
	height:56px;
	bottom:0px;
	padding:0px 0px 0px 0px;
	margin:0px;
	display:block;
	text-indent:180px;
	background-color:#fff;
	z-index:1301;
	
}



.form-content  form.form-horizontal .form-actions .btn{
	margin:14px 0px;
	
}


