body{overflow-y:none;}
#main {padding:0;margin:0;background: #eff0f4 none repeat scroll 0 0;} 
#main .container-fluid{padding:0 4px 0 0px;} 
#main .container-fluid #content #right{margin:0px 0px 0px 180px;position:relative;background: #eff0f4 none repeat scroll 0 0;}
#right #jerichotab{overflow:hidden;}
#header {margin:0 0 0px 180px;position:relative;height: 40px;} 
#header .tooltips{
	height: 40px;
    margin-left: 20px;   
    display: inline-block;
    cursor: pointer;
    font-size: 20px;
    line-height:40px;
    
}
#header .navbar-inner{
	height: 40px;
	min-height: 40px;
	
}
.navbar-inner ul#userControl>li{height:40px; line-height:40px;}
.navbar-inner ul#userControl>li>a{padding:0px 15px;}
#header .navbar-inner{background-color:#fff;border:none;background-image:none;}
#header li {font-size:14px;_font-size:12px;}
#openClose,#left{overflow-x:hidden;overflow-y:auto;position:fixed;top:0px;z-index:1029;} 
#openClose{margin-left:-10px;}
#left .collapse{position:static;}

#left .brand,
body .brand {
	text-align:center;
}
#content>.brand{
	position:fixed;
	top:0px;
	left:0px;
	z-index:1109;
}
#left .brand a,
body .brand a{	
	position:relative;
	display:block;
	width:180px;	
	height:50px;
	magrin:0px 0px 6px 0px;
	padding:0px;	
	font-family:Helvetica, Georgia, Arial, sans-serif, 榛戜綋;
    font-size:20px;
    font-weight:bold;
    letter-spacing:1px;
    line-height:50px;
    color:#eee;    
	text-align:left;
	background:#3b5e8e;
    }
#left .brand>a>img,
body .brand>a>img{
	width:36px;
	height:36px;
	margin-left:10px;
	vertical-align: middle;
}
#left div.brand a:hover,body div.brand a:hover{
	color:#fff;
	text-decoration:none;
	background-color:none !important;
	background:#3b5e8e;
}
#footer {margin:8px 0 0 0;padding:3px 0 0 0;font-size:11px;text-align:center;border-top:2px solid #0663A2;}
#footer, #footer a {color:#999;} 
#userControl>li>a{color:#58666e;text-shadow:none;} #userControl>li>a:hover, #user #userControl>li.open>a{background:transparent;}

#userControl>li>a:hover{
	background-color:rgba(0, 0, 0, 0.2);
}

#left{
	background:#404040;
	text-align:center;
	margin:0px;
	padding:0px;
}

#left a{
	background:#505050 none repeat scroll 0 0;
    font-family:Helvetica,鈥婫eorgia,鈥婣rial,鈥媠ans-serif,鈥嬪畫浣�;
    font-size:12px;
    font-weight:400;
	color:#fff;
	text-decoration:none;
	line-height:30px;
    border:0 none;
    border-radius:0 !important;
    display:block;
    font-size:12px;
    font-weight:normal;
    margin:0;
    padding:5px 0px 5px 5px;
    position:relative;
    text-align:left;
    text-decoration:none;
    transition-duration:500ms;
    transition-property:width, background;
    transition-timing-function:ease;
    width:175px;
    }

#left a.active{
	background:#4a8bc2 none repeat scroll 0 0 !important;
}


#openClose{
	display:none;
}

#left .accordion-group{
	margin-bottom:1px;
	width:100%;
	height:auto;
	background:#404040;
	color:#fff;
	border:0px;
	border-radius:0px;
}

#left .accordion-group .accordion-body .accordion-inner{
	border:0px;
}
#left .accordion-group .accordion-body .accordion-inner a{
	background:#404040;
}
#left .accordion-group a i{
	width:20px;
	font-size:14px;
}
#left .accordion-group .accordion-body .accordion-inner,
#left .accordion-group .accordion-body .accordion-inner ul.nav-list,
#left .accordion-group .accordion-body .accordion-inner ul.nav-list li,
#left .accordion-group .accordion-body .accordion-inner ul.nav-list li ul li,
#left ul li{
	margin:0px;
	padding:0px;
}
#left .accordion-group .accordion-body .accordion-inner ul.nav-list li a{
	padding-left:20px;
	width:160px;
}



#left .accordion-group .accordion-body .accordion-inner ul.nav-list li ul,
#left .accordion-group .accordion-body .accordion-inner ul.nav-list li ul li,
#left .accordion-group .accordion-body .accordion-inner ul.nav-list li ul li a{
	background-color:#3D3535;
	
}

#left .accordion-group .accordion-body .accordion-inner ul.nav-list li ul li a{
	padding-left:30px;
	width:150px;
	
}








