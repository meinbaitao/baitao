package com.bt.modules.orbom.bigbean;

import java.util.ArrayList;
import java.util.List;

import com.bt.modules.orbom.entity.OrderBomBase;
import com.bt.modules.orbom.entity.OrderBomBox;
import com.bt.modules.orbom.entity.OrderBomDetail;

public class OrderBomInfo {
	OrderBomBase orderBomBase;
	List<OrderBomDetail> listOrderBomDetails = new ArrayList<OrderBomDetail>();
	List<OrderBomBox> listBoxs = new ArrayList<OrderBomBox>();
	
	public OrderBomBase getOrderBomBase() {
		return orderBomBase;
	}
	public void setOrderBomBase(OrderBomBase orderBomBase) {
		this.orderBomBase = orderBomBase;
	}
	public List<OrderBomDetail> getListOrderBomDetails() {
		return listOrderBomDetails;
	}
	public void setListOrderBomDetails(List<OrderBomDetail> listOrderBomDetails) {
		this.listOrderBomDetails = listOrderBomDetails;
	}
	public List<OrderBomBox> getListBoxs() {
		return listBoxs;
	}
	public void setListBoxs(List<OrderBomBox> listBoxs) {
		this.listBoxs = listBoxs;
	}
	
}
